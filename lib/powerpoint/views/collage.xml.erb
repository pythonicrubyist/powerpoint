        </p:spPr>
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:pPr algn="l"/>
            <a:r>
              <a:rPr lang="en-US" b="1" dirty="0" sz="2800" >
                <a:solidFill>
                  <a:schemeClr val="dk2" />
                </a:solidFill>
                <a:latin typeface="Arvo" panose="02000000000000000000" pitchFamily="2" charset="77"/>
              </a:rPr>
              <a:t><%= title %></a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="3" name="subtitle"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr />
